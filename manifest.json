{
  "manifest_version": 3,
  "name": "My First Extension",
  "version": "1.0",
  "description": "Simple Chrome Extension Boilerplate",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
